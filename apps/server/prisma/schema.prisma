// Prisma Schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Data {
  id        String   @id @default(uuid()) // id
  clientId  String // 客户端ID
  topic     String // 主题
  type      Int      // 类型
  module    Json     // 模块数据
  createdAt DateTime @default(now()) // 创建时间
  updatedAt DateTime @updatedAt // 更新时间
}
